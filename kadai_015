class Human:
    def __init__(self):
        self.name = ""
        self.age = ""

    def set_name(self, name, age):
        self.name = name
        self.age = age       

    def printinfo(self):
        print(self.name)
        print(self.age)

Man = Human()
Man.set_name("侍太郎", 36)
Man.printinfo()
